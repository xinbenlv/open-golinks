    "vee-validate": "^3.3.0",
    "vue-clipboard2": "^0.3.1"
  },
  "engines": {
    "node": "12.18.0",
    "npm": "6.14.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      ".nuxt",
      ".idea"
    ],
    "watch": [
      "src",
      "static",
      "nuxt.config.js",
      "package.json",
      "vue-shim.d.ts",
      "tsconfig.json"
    ],
    "exec": "npm run dev:node",
