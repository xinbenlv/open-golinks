{
  "name": "open-golinks",
  "version": "2.1.4",
  "description": "An web app providing go-links service.",
  "main": "src/main.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "nuxt-ts build",
    "dev:node": "npx ts-node -r tsconfig-paths/register --project tsconfig.json src/main.ts",
    "dev": "nodemon",
    "debug": "nodemon",
    "nuxt": "nuxt",
    "start": "cross-env NODE_ENV=production npx ts-node -r tsconfig-paths/register --project tsconfig.json src/main.ts",
    "prod": "npm run build && npm run dev:node",
    "heroku-postbuild": "npm run build",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version"
  },
  "husky": {
    "hooks": {
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xinbenlv/open-golinks.git"
  },
  "keywords": [
    "golink",
    "go-link",
    "short",
    "url"
  ],
  "author": "Zainan Victor Zhou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xinbenlv/open-golinks/issues"
  },
  "homepage": "https://github.com/xinbenlv/open-golinks#readme",
  "dependencies": {
    "@nuxt/types": "^2.18.1",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/axios": "^5.13.6",
    "axios": "^0.27.2",
    "body-parser": "^2.2.0",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "canvas": "^3.1.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.7",
    "core-js": "^2.6.12",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "express-status-monitor": "^1.3.4",
    "google-auth-library": "^9.15.1",
    "jimp": "^1.6.0",
    "jquery": "^3.7.1",
    "log4js": "^6.9.1",
    "mongodb": "^3.7.3",
    "mongoose": "^5.13.17",
    "node-cache": "^5.1.2",
    "nuxt": "^2.16.3",
    "nuxt-env": "^0.1.0",
    "nuxt-property-decorator": "^2.9.1",
    "passport": "^0.4.1",
    "passport-auth0": "^1.4.4",
    "qrcode": "^1.5.4",
    "regenerator-runtime": "^0.14.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ts-mongoose": "^0.0.24",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "validator": "^13.15.0",
    "vee-validate": "^3.4.15",
    "vue": "^2.7.16",
    "vue-clipboard2": "^0.3.3",
    "vue-property-decorator": "^9.1.2",
    "vue-server-renderer": "^2.7.16",
    "vue-template-compiler": "^2.7.16"
  },
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      ".nuxt",
      ".idea"
    ],
    "watch": [
      "src",
      "static",
      "nuxt.config.js",
      "package.json",
      "vue-shim.d.ts",
      "tsconfig.json"
    ],
    "exec": "npm run dev:node",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.14.0",
    "@types/qrcode": "^1.5.5",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "standard-version": "^9.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
