    "@nuxt/typescript-runtime": "^0.4.2",
    "@nuxtjs/axios": "^5.9.7",
    "@types/mongoose": "^5.7.14",
    "@types/qrcode": "^1.3.4",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.10.1",
    "canvas": "^2.6.1",
