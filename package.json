    "date-fns": "^2.12.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-status-monitor": "^1.1.5",
    "google-auth-library": "^6.0.0",
    "jimp": "^0.13.0",
    "log4js": "^6.0.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.7.14",
    "node-cache": "^5.0.0",
    "nuxt": "^2.12.2",
    "nuxt-env": "^0.1.0",
    "nuxt-property-decorator": "^2.7.2",
