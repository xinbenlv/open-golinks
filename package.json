{
  "name": "open-golinks",
  "version": "2.0.1-alpha.0",
  "description": "An web app providing go-links service.",
  "main": "src/main.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "nuxt-ts build",
    "dev:node": "npx ts-node -r tsconfig-paths/register --project tsconfig.json src/main.ts",
    "dev": "nodemon",
    "debug": "nodemon",
    "nuxt": "nuxt",
    "start": "cross-env NODE_ENV=production npx ts-node -r tsconfig-paths/register --project tsconfig.json src/main.ts",
    "prod": "npm run build && npm run dev:node",
    "heroku-postbuild": "npm run build",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xinbenlv/open-golinks.git"
  },
  "keywords": [
    "golink",
    "go-link",
    "short",
    "url"
  ],
  "author": "Zainan Victor Zhou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xinbenlv/open-golinks/issues"
  },
  "homepage": "https://github.com/xinbenlv/open-golinks#readme",
  "dependencies": {
    "@jimp/plugin-print": "^0.14.0",
    "@nuxt/types": "^2.13.3",
    "@nuxt/typescript-build": "^2.0.0",
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/axios": "^5.12.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "canvas": "^2.6.1",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "core-js": "^2.6.11",
    "cross-env": "^7.0.2",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-status-monitor": "^1.2.11",
    "google-auth-library": "^6.0.5",
    "jimp": "^0.14.0",
    "jquery": "^3.5.1",
    "log4js": "^6.3.0",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.24",
    "node-cache": "^5.1.2",
    "nuxt": "^2.13.3",
    "nuxt-env": "^0.1.0",
    "nuxt-property-decorator": "^2.7.2",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.2",
    "qrcode": "^1.4.4",
    "query-string": "^6.13.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "request-promise-native": "^1.0.8",
    "sharp": "^0.25.4",
    "snyk": "^1.361.3",
    "ts-mongoose": "0.0.21",
    "ts-node": "^8.10.2",
    "tsc": "^1.20150623.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6",
    "universal-analytics": "^0.4.23",
    "uuid": "^8.2.0",
    "validator": "^13.1.1",
    "vee-validate": "^3.3.7",
    "vue-clipboard2": "^0.3.1"
  },
  "engines": {
    "node": "12.18.2",
    "npm": "6.14.6"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      ".nuxt",
      ".idea"
    ],
    "watch": [
      "src",
      "static",
      "nuxt.config.js",
      "package.json",
      "vue-shim.d.ts",
      "tsconfig.json"
    ],
    "exec": "npm run dev:node",
    "ext": "ts"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@types/express": "4.17.7",
    "@types/express-serve-static-core": "4.17.8",
    "@types/jquery": "3.5.0",
    "@types/mongoose": "5.11.96",
    "@types/node": "13.13.14",
    "@types/qrcode": "1.3.4",
    "@types/universal-analytics": "0.4.4",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "husky": "4.2.5",
    "nodemon": "2.0.4",
    "standard-version": "8.0.2",
    "webpack-hot-middleware": "2.25.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
